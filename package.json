{
  "name": "node-ovh-ts",
  "version": "0.0.4",
  "description": "OVH API wrapper library for TypeScript",
  "license": "MIT",
  "homepage": "https://github.com/BFriedrichs/node-ovh-ts",
  "author": "Bj√∂rn Friedrichs <mail@bjornf.dev>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src -- --dts-resolve",
    "build:fast": "tsup src",
    "dev": "tsup src --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "size": "size-limit",
    "prettier": "prettier",
    "coverage": "vitest run --coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix && pnpm format",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ovh",
    "typescript",
    "api"
  ],
  "devDependencies": {
    "@vitest/coverage-c8": "^0.24.5",
    "happy-dom": "^6.0.4",
    "@commitlint/config-conventional": "^17.4.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitlint": "^17.4.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "size-limit": "^8.1.0",
    "tsup": "^6.5.0",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.24.5"
  },
  "packageManager": "pnpm@7.9.0",
  "size-limit": [
    {
      "path": "./lib/dist/index.js",
      "limit": "15 kb"
    },
    {
      "path": "./lib/dist/index.mjs",
      "limit": "15 kb"
    }
  ]
}
